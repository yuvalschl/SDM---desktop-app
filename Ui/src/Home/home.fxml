<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1020.0" style="-fx-background-color: #fffffc;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Home.HomeController">
   <children>
      <VBox layoutX="236.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="500.0" spacing="100.0">
         <children>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <HBox alignment="CENTER" spacing="60.0">
                     <children>
                        <Button fx:id="loadXmlButton" mnemonicParsing="false" onAction="#loadXmlAction" text="Load XML" />
                        <Button fx:id="chooseFileButton" lineSpacing="10.0" mnemonicParsing="false" onAction="#chooseFileAction" text="Chosse File" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="26.0" prefWidth="260.0">
                     <children>
                        <StackPane prefHeight="26.0" prefWidth="283.0">
                           <children>
                              <ProgressBar fx:id="fileProgressBar" prefHeight="23.0" prefWidth="272.0" progress="0.0" />
                              <Label fx:id="progressPercentText" />
                           </children>
                        </StackPane>
                     </children>
                  </HBox>
                  <Text fx:id="loadActionText" fill="#101011" strokeType="OUTSIDE" strokeWidth="0.0" text="Loading file: ">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Text>
               </children>
            </VBox>
            <ScrollPane fx:id="mapScrollPane" prefHeight="480.0" prefWidth="480.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                         <fx:include fx:id="mapGrid" source="Map/map.fxml" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
