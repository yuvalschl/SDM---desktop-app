<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1020.0" style="-fx-background-color: #fffffe;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Home.HomeController">
   <children>
      <VBox layoutX="380.0" layoutY="153.0">
         <children>
            <HBox alignment="CENTER" spacing="60.0">
               <children>
                  <Button fx:id="loadXmlButton" mnemonicParsing="false" onAction="#loadXmlAction" text="Load XML" />
                  <Button lineSpacing="10.0" mnemonicParsing="false" text="Chosse File" />
               </children>
            </HBox>
            <HBox prefHeight="26.0" prefWidth="260.0">
               <children>
                  <StackPane prefHeight="26.0" prefWidth="283.0">
                     <children>
                        <ProgressBar fx:id="fileProgressBar" prefHeight="23.0" prefWidth="272.0" progress="0.0" />
                        <Label fx:id="progressPercentText" />
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <Text fx:id="loadActionText" fill="#101011" strokeType="OUTSIDE" strokeWidth="0.0" text="Loading file: ">
               <font>
                  <Font size="17.0" />
               </font>
            </Text>
         </children>
      </VBox>
   </children>
</AnchorPane>
